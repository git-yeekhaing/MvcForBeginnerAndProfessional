@model myExercises.Models.Employee

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width"/>
    <title>Index</title>
</head>
<body>

    @{ 
        var employee = ViewData["Employee"] as myExercises.Models.Employee;
    }

    <h2>@ViewData["Header"]</h2>
    <table style="font-family:Arial">
        <tr>
            <td>Employee Id:</td>
            <td>@Model.EmployeeId</td>
        </tr>
        <tr>
            <td>Name:</td>
